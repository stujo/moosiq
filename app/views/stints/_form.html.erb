<%= simple_form_for(@stint) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :band %>
    <%= f.association :artist %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
